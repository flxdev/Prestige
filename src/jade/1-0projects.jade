                    img.parallax(src='img/projects/small.jpg', alt='', data-speed='15', data-direction='-1')
                  .project__name
                    .name
                      span woodtable
                    .category interior design
              .projects-col.interior.parallax(data-speed='15')
                a.project__item.pos-bottom(href='')
                  .project__media
                    img.parallax(src='img/projects/small.jpg', alt='', data-speed='15', data-direction='-1')
                  .project__name
                    .name
