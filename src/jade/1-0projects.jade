doctype html
head
  title 1-0projects
  include ./_head.jade
.out
  include ./_frame.jade
  .frame.frame_left
    .breadcrumbs-container
      .breadcrumbs-inner
        ul.breadcrumbs-list
          li
            a(href='#') Home
  include ./_grid.jade
  .wrapper
    section.section
      .section__head.head__projects
        .section_container
          .lead.parallax(data-speed='5')
            .title # projects
      .container
        .projects
          .filter
            .filter__container.parallax(data-speed='5')
              a.filter__item.active(href='#all',data-filter='*')
                span All projects
                span.counter 38
              a.filter__item(href='#interior',data-filter='.interior')
                span Interior design
                span.counter 25
              a.filter__item(href='#design',data-filter='.design')
                span Design furniture
                span.counter 13
          .projects__container
            .project__grid
              .projects-col.interior.parallax(data-speed='15')
                a.project__item.pos-bottom(href='')
                  .project__media
                    img.parallax(src='img/projects/small.jpg', alt='', data-speed='15', data-direction='-1')
                  .project__name
                    .name
                      span woodtable
                    .category interior design
              .projects-col.design.parallax(data-speed='25')
                a.project__item.item__big(href='')
                  .project__media
                    img.parallax(src='img/projects/small.jpg', alt='', data-speed='15', data-direction='-1')
                  .project__name
                    .name
                      span Toilet in
                      br
                      span the alfabank
                    .category interior design
              .projects-col.design.parallax(data-speed='15')
                a.project__item.pos-top(href='')
                  .project__media
                    img.parallax(src='img/projects/small.jpg', alt='', data-speed='15', data-direction='-1')
                  .project__name
                    .name
                      span Big brother
                      br
                      span is watching you
                    .category design design
              .projects-col.interior.parallax(data-speed='25')
                a.project__item.pos-top(href='')
